    <!-- Section: jobs -->
              
    <section  class="section section-popular scrollspy">
        <div class="container">
            <div class="row">
          {{#if results}}
            
             {{#each results}}
                <div class="col s12 m6 l4">
                    <div class="card hoverable">
                        <div class="card-image small"> 
                            {{#if this.image}}
                            <img src="{{this.image}}" alt="">
                            {{else}}
                            <img src="/images/no_job_image_a.png" alt="">
                            {{/if}}
                            {{#if start_time}}
                                <span class=" start_time_back font_weight_bold  ">{{start_time}}</span>
                           {{/if}}
                           
                        </div>
                       
                        <div class="card-content card-content-jobs">
                               <p>
                                    <b>  
                                        {{#if salary}}
                                        <span class="salary-card blue-text">{{salary}} LEI  </span>|
                                        {{/if}}
                                        <span>{{employment_type}}</span>
                                    </b>
                                </p>
                            
                                <p class="card-position"> <b>{{position}}</b> </p>
                                <p class="truncate description text-opacity" >{{description}}</p>


                                <p class="text-opacity"><i class="material-icons">place</i>{{city}}</p>

                         </div>
                    
                            <div class="card-action">
                                <a href="/job/details/{{id}}" class=" btn white-text text-lighten-1 waves-effect waves-light"> Vezi Mai mult</a>

                            </div>
                   </div>
                </div>
            {{/each}}
           {{else}}
           <h4  class="center">Nici un job nu este disponibil la moment</h4>
         {{/if}}
        </div>
    </div>
</section>


